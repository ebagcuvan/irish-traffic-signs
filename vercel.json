{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/public/data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/sitemap",
      "schedule": "0 */6 * * *"
    }
  ]
}
